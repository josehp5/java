public class PracticeProblems {
    public static int[] reverseArray(int[] arr) {
        int[] reversedarray = new int[arraylength];
        int j = 0;

        for (int i = arraylength - 1; i >= 0; i--) {
            reversedarray[j] = arr[i];
            j++;
        }

        return reversedarray;
    }
}
